    "import asyncio\n",
    "\n",
    "print(\"root start\")\n",
    "# 쥬피터는 최상위에서 await 사용 가능하다.\n",
    "await asyncio.sleep(2)\n",
    "\n",
    "\n",
    "async def main():\n",
    "    print(\"main start\")\n",
    "    await asyncio.sleep(1)\n",
    "    print(\"main end\")\n",
    "\n",
    "\n",
    "asyncio.run(main())"
   ]
